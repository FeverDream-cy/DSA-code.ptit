#include<iostream>
#include<queue>
#include<string>
using namespace std;
typedef long long ll;
int res = 0;
ll str(string s) {
	ll x = 0;
	for (int i = 0; i < s.length(); i++)
		x = x * 10 + s[i] - 48;
	return x;
}
void load(ll n) {
	string d = to_string(n);
	queue<string> S;
	S.push("1");
	int l = 0;
	while (l<=d.length()) {
		string s = S.front();
		S.pop();
		if (str(s) <= n)res++; 
		else return;
		S.push(s + "0");
		S.push(s + "1");
		l = S.front().length();
	}
}
int main() {
	int t; cin >> t;
	while (t--) {
		ll n; cin >> n;
		res = 0;
		load(n);
		cout << res << endl;
	}
}
